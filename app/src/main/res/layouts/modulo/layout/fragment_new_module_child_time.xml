<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".common.modulo.NewModuleChildTimeFragment"
    android:background="@color/white"
    android:paddingLeft="30dp"
    android:paddingTop="20dp"
    android:paddingBottom="20dp"
    android:paddingRight="60dp">

   <LinearLayout
       android:layout_width="match_parent"
       android:layout_height="wrap_content"
       android:orientation="vertical"
       android:layout_gravity="center">

       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:paddingVertical="@dimen/icon_4dp"
           android:orientation="vertical">

           <LinearLayout
               android:id="@+id/semana_fumigacion_modulo_layout"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:background="@drawable/rounder_corner"
               android:padding="@dimen/icon_4dp">

               <LinearLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:orientation="horizontal"
                   android:padding="@dimen/google_1x">

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/ic_baseline_create_24"
                       android:tint="@color/blue_green"/>

                   <LinearLayout
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="vertical">

                       <com.google.android.material.textview.MaterialTextView
                           android:id="@+id/semana_fumigacion_modulo"
                           android:layout_marginLeft="16dp"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:textSize="16dp"
                           android:text="Indique semanas de fumigación :"
                           android:textColor="@color/blue_green"
                           style="@style/ThemeMaterialCalendar"/>

                       <com.androidbuts.multispinnerfilter.MultiSpinnerSearch
                           android:id="@+id/mssFumigaciónModulo"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_margin="@dimen/google_1x"
                           app:textColor="@color/prussian_blue" />
                   </LinearLayout>



               </LinearLayout>



           </LinearLayout>


       </LinearLayout>

       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:paddingVertical="@dimen/icon_4dp"
           android:orientation="vertical">

           <LinearLayout
               android:id="@+id/semana_limpieza_modulo_layout"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:background="@drawable/rounder_corner"
               android:padding="@dimen/icon_4dp">

               <LinearLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:orientation="horizontal"
                   android:padding="@dimen/google_1x">

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/ic_baseline_create_24"
                       android:tint="@color/blue_green"/>

                   <LinearLayout
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="vertical">

                       <com.google.android.material.textview.MaterialTextView
                           android:id="@+id/semana_limpieza_modulo"
                           android:layout_marginLeft="16dp"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:textSize="16dp"
                           android:text="Indique semanas de limpieza :"
                           android:textColor="@color/blue_green"
                           style="@style/ThemeMaterialCalendar"/>

                       <com.androidbuts.multispinnerfilter.MultiSpinnerSearch
                           android:id="@+id/mssLimpiezaModulo"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_margin="@dimen/google_1x"
                           app:textColor="@color/prussian_blue" />
                   </LinearLayout>



               </LinearLayout>



           </LinearLayout>


       </LinearLayout>

       <LinearLayout
           android:layout_width="match_parent"
           android:layout_height="wrap_content"
           android:paddingVertical="@dimen/icon_4dp"
           android:orientation="vertical">

           <LinearLayout
               android:id="@+id/semana_cosecha_modulo_layout"
               android:layout_width="match_parent"
               android:layout_height="wrap_content"
               android:orientation="horizontal"
               android:background="@drawable/rounder_corner"
               android:padding="@dimen/icon_4dp">

               <LinearLayout
                   android:layout_width="match_parent"
                   android:layout_height="wrap_content"
                   android:orientation="horizontal"
                   android:padding="@dimen/google_1x">

                   <ImageView
                       android:layout_width="wrap_content"
                       android:layout_height="wrap_content"
                       android:src="@drawable/ic_baseline_create_24"
                       android:tint="@color/blue_green"/>


                   <LinearLayout
                       android:layout_width="match_parent"
                       android:layout_height="wrap_content"
                       android:orientation="vertical">

                       <com.google.android.material.textview.MaterialTextView
                           android:id="@+id/semana_cosecha_modulo"
                           android:layout_marginLeft="16dp"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:textSize="16dp"
                           android:text="Indique semanas de cosecha :"
                           android:textColor="@color/blue_green"
                           style="@style/ThemeMaterialCalendar"/>

                       <com.androidbuts.multispinnerfilter.MultiSpinnerSearch
                           android:id="@+id/mssCosechaModulo"
                           android:layout_width="match_parent"
                           android:layout_height="wrap_content"
                           android:layout_margin="@dimen/google_1x"
                           app:textColor="@color/prussian_blue" />
                   </LinearLayout>



               </LinearLayout>



           </LinearLayout>


       </LinearLayout>


   </LinearLayout>

</FrameLayout>